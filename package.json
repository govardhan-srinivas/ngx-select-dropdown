{
  "name": "custom-select-dropdown",
  "version": "1.0.14",
  "description": "A angular(4+) selct dropdown for single selct or multiselct module.",
  "main": "./dist/index.umd.js",
  "typings": "./dist/index.d.ts",
  "license": "MIT",
  "keywords": [
    "angular",
    "angular-2",
    "angular-4",
    "angular2",
    "angular4",
    "ng2",
    "ng4",
    "ngx",
    "select",
    "dropdown",
    "multiselect",
    "multi-select",
    "dropdown",
    "custom",
    "combobox",
    "ng-select",
    "ngx-select",
    "ng2-select",
    "ng-dropdown",
    "ngx-dropdown",
    "ng2-dropdown"
  ],
  "author": {
    "name": "Govardhan Srinivas",
    "email": "govardhan-s92@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/govardhan-srinivas/ngx-select-dropdown.git"
  },
  "bugs": {
    "url": "https://github.com/govardhan-srinivas/ngx-select-dropdown/issues"
  },
  "homepage": "https://github.com/govardhan-srinivas/ngx-select-dropdown#readme",
  "scripts": {
    "build": "npm run build:esm && npm run build:umd",
    "build:esm": "gulp inline-templates && npm run ngcompile",
    "build:esm:watch": "gulp build:esm:watch",
    "build:umd": "webpack --config webpack-umd.config.ts",
    "build:umd:watch": "npm run build:umd -- --watch",
    "build:watch": "concurrently --raw \"npm run build:umd:watch\" \"npm run build:esm:watch\"",
    "ci": "npm run lint && npm run test && npm run build",
    "clean:all": "npm run clean:tmp && rimraf node_modules",
    "clean:tmp": "rimraf coverage dist tmp",
    "codecov": "cat coverage/lcov.info | codecov",
    "docs": "compodoc -p tsconfig.json -d docs --disableCoverage --disablePrivateOrInternalSupport",
    "explorer": "source-map-explorer ./dist/index.umd.js",
    "lint": "npm run tslint 'src/**/*.ts'",
    "ngcompile": "node_modules/.bin/ngc -p tsconfig-aot.json",
    "postversion": "git push && git push --tags",
    "prebuild": "rimraf dist tmp",
    "prepare": "npm run build",
    "prebuild:watch": "rimraf dist tmp",
    "test": "karma start",
    "test:watch": "karma start --auto-watch --no-single-run",
    "tslint": "tslint",
    "compodoc": "compodoc -p tsconfig.json"
  },
  "dependencies": {
    "node-gyp": "^3.8.0"
  },
  "devDependencies": {
    "@angular/common": "^4.4.7",
    "@angular/compiler": "^4.4.7",
    "@angular/compiler-cli": "^4.4.7",
    "@angular/core": "^4.4.7",
    "@angular/forms": "^4.4.7",
    "@angular/platform-browser": "^4.4.7",
    "@angular/platform-browser-dynamic": "^4.4.7",
    "@compodoc/compodoc": "^1.1.10",
    "@types/jasmine": "^2.8.16",
    "@types/karma": "^1.7.8",
    "@types/node": "^8.10.51",
    "@types/webpack": "^3.8.18",
    "@types/webpack-env": "^1.14.0",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^3.5.0",
    "codecov": "^2.2.0",
    "codelyzer": "^3.0.1",
    "concurrently": "^3.6.1",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^0.28.11",
    "gh-pages": "^1.2.0",
    "gulp": "^3.9.1",
    "gulp-inline-ng2-template": "^4.0.0",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jasmine-core": "^2.99.1",
    "json-loader": "^0.5.4",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.1.1",
    "karma-coverage-istanbul-reporter": "^1.4.3",
    "karma-jasmine": "^1.1.2",
    "karma-mocha-reporter": "^2.2.3",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.13",
    "node-sass": "^4.12.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.3",
    "rxjs": "^5.5.12",
    "sass-loader": "^6.0.7",
    "source-map-explorer": "^1.8.0",
    "to-string-loader": "^1.1.5",
    "ts-node": "^3.0.4",
    "tslint": "^5.18.0",
    "typescript": "^2.9.2",
    "webpack": "^3.12.0",
    "webpack-angular-externals": "^1.0.2",
    "webpack-rxjs-externals": "^1.0.0",
    "zone.js": "^0.8.29"
  }
}
